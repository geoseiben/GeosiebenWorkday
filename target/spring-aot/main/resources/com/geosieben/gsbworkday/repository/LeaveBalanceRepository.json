      "name": "findByEmployeeBasicInfo_EID",
      "signature": "public abstract com.geosieben.gsbworkday.entity.LeaveBalance com.geosieben.gsbworkday.repository.LeaveBalanceRepository.findByEmployeeBasicInfo_EID(java.lang.String)",
      "query": {
        "query": "SELECT l FROM LeaveBalance l WHERE l.employeeBasicInfo.EID = :eid"
      }
    },
    {
      "name": "findLeaveBalanceByEid",
      "signature": "public abstract com.geosieben.gsbworkday.dto.LeaveBalanceReposnseDto com.geosieben.gsbworkday.repository.LeaveBalanceRepository.findLeaveBalanceByEid(java.lang.String)",
      "query": {
        "query": "SELECT new com.geosieben.gsbworkday.dto.LeaveBalanceReposnseDto(e.EID, CONCAT(e.firstName, ' ', e.lastName), lb.casualleaves, lb.sickleaves, lb.restrictedholidays, lb.earnedleaves) FROM LeaveBalance lb JOIN lb.employeeBasicInfo e WHERE e.EID = :eid"
      }
    },
